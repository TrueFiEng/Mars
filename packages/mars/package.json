{
  "name": "ethereum-mars",
  "description": "The amazing smart contract deployment tool",
  "license": "MIT",
  "version": "0.1.3",
  "keywords": [
    "deployment",
    "ethereum",
    "smart",
    "contracts",
    "javascript",
    "typescript",
    "library"
  ],
  "homepage": "https://github.com/EthWorks/Mars",
  "bugs": {
    "url": "https://github.com/EthWorks/Mars/issues"
  },
  "files": [
    "build",
    "src",
    "bin.js"
  ],
  "engines": {
    "node": ">=10",
    "yarn": "^1.17.3"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "bin": {
    "mars": "bin.js"
  },
  "scripts": {
    "lint": "yarn lint:prettier --check && yarn lint:eslint",
    "lint:fix": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:eslint": "eslint './{src,test}/**/*.ts'",
    "lint:prettier": "prettier './{src,test}/**/*.ts'",
    "build": "yarn build:test:contracts && tsc",
    "build:test:contracts": "waffle ./test/waffle.json",
    "test": "mocha",
    "prepublishOnly": "rm -rf build && yarn build"
  },
  "dependencies": {
    "@resolver-engine/imports": "^0.3.3",
    "@resolver-engine/imports-fs": "^0.3.3",
    "argparse": "^2.0.1",
    "axios": "^0.21.0",
    "ethers": "^5.0.24",
    "ganache-core": "^2.13.2",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.4",
    "@types/chai": "^4.2.14",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^8.2.0",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "eslint": "^7.15.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
